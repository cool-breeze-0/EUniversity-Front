<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/appBackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="18dp">

            <Button
                android:onClick="onClick"
                android:id="@+id/userProfileInformationBackButton"
                android:layout_width="@dimen/icon_arrow_left"
                android:layout_height="@dimen/icon_arrow_left"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="true"
                android:background="@drawable/icon_arrow_left" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/user_profile_information"
                android:textSize="@dimen/actionbar_text_size" />
        </RelativeLayout>
    </androidx.appcompat.widget.Toolbar>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@drawable/shape_cornors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="0.12">

        <TextView
            android:id="@+id/textViewUserProfileInformationTitleName"
            android:layout_width="@dimen/user_profile_information_title_width"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/user_profile_information_textview_padding"
            android:paddingTop="@dimen/user_profile_information_textview_padding"
            android:paddingRight="@dimen/user_profile_information_textview_padding"
            android:paddingBottom="@dimen/user_profile_information_textview_padding"
            android:text="@string/user_profile_information_name"
            android:textSize="@dimen/user_profile_information_title_text_size" />

        <EditText
            android:id="@+id/editTextUserProfileInformationName"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:singleLine="true"
            android:background="@null"
            android:ems="10"
            android:textSize="@dimen/user_profile_information_title_text_size" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginTop="@dimen/user_input_view_top_to_title"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@drawable/shape_cornors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textViewUserProfileInformationTitleGender"
            android:layout_width="@dimen/user_profile_information_title_width"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/user_profile_information_textview_padding"
            android:paddingTop="@dimen/user_profile_information_textview_padding"
            android:paddingRight="@dimen/user_profile_information_textview_padding"
            android:paddingBottom="@dimen/user_profile_information_textview_padding"
            android:text="@string/user_profile_information_gender"
            android:textSize="@dimen/user_profile_information_title_text_size" />

        <TextView
            android:id="@+id/textViewUserProfileInformationGender"
            android:onClick="onClick"
            android:gravity="center_vertical|left"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/user_profile_information_title_text_size" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginTop="@dimen/user_input_view_top_to_title"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@drawable/shape_cornors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textViewUserProfileInformationTitleProvince"
            android:layout_width="@dimen/user_profile_information_title_width"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/user_profile_information_textview_padding"
            android:paddingTop="@dimen/user_profile_information_textview_padding"
            android:paddingRight="@dimen/user_profile_information_textview_padding"
            android:paddingBottom="@dimen/user_profile_information_textview_padding"
            android:text="@string/user_profile_information_province"
            android:textSize="@dimen/user_profile_information_title_text_size" />

        <TextView
            android:id="@+id/textViewUserProfileInformationProvince"
            android:onClick="onClick"
            android:gravity="center_vertical|left"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/user_profile_information_title_text_size" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginTop="@dimen/user_input_view_top_to_title"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@drawable/shape_cornors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textViewUserProfileInformationTitleCity"
            android:layout_width="@dimen/user_profile_information_title_width"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/user_profile_information_textview_padding"
            android:paddingTop="@dimen/user_profile_information_textview_padding"
            android:paddingRight="@dimen/user_profile_information_textview_padding"
            android:paddingBottom="@dimen/user_profile_information_textview_padding"
            android:text="@string/user_profile_information_city"
            android:textSize="@dimen/user_profile_information_title_text_size" />

        <EditText
            android:id="@+id/textViewUserProfileInformationCity"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:singleLine="true"
            android:ems="10"
            android:textSize="@dimen/user_profile_information_title_text_size" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginTop="@dimen/user_input_view_top_to_title"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@drawable/shape_cornors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textViewUserProfileInformationTitleIdentity"
            android:layout_width="@dimen/user_profile_information_title_width"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/user_profile_information_textview_padding"
            android:paddingTop="@dimen/user_profile_information_textview_padding"
            android:paddingRight="@dimen/user_profile_information_textview_padding"
            android:paddingBottom="@dimen/user_profile_information_textview_padding"
            android:text="@string/user_profile_information_identity"
            android:textSize="@dimen/user_profile_information_title_text_size" />

        <TextView
            android:id="@+id/textViewUserProfileInformationIdentity"
            android:onClick="onClick"
            android:gravity="center_vertical|left"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/user_profile_information_title_text_size" />
    </LinearLayout>
    <Button
        android:id="@+id/finishButton"
        android:onClick="onClick"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/user_input_view_top_to_title"
        android:layout_marginStart="@dimen/universal_margin_left_right"
        android:layout_marginEnd="@dimen/universal_margin_left_right"
        android:background="@color/buttonColor"
        android:text="@string/finish"
        android:textColor="@color/white"
        android:textSize="@dimen/user_widget_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>